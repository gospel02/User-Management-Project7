<h3> User List </h3>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
           <th>First Name</th>
           <th>Last Name</th>
           <th>Email</th>
           <th>Age</th>
           <th>Edit</th>
           <th>Delete</th>
            
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.firstName}}</td>
            <td>{{this.lastName}}</td>
            <td>{{this.email}}</td>
            <td>{{this.age}}</td>
            <td>
                <a href="/user/{{this._id}}" class="btn btn-secondary">Edit</a>
            </td>
            <td>
                <a href="/user/delete/{{this._id}}" class="btn btn-secondary">Delete</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
<a class="btn btn-secondary" href="/user"> Add User </a>